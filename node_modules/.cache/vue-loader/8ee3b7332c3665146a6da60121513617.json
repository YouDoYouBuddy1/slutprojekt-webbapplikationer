"header\"><h1> Title: {{product.title}}</h1> </div>\n        \n        \n        <div class=\"sidebar\"><!-- sidebar  -->\n          \n          <span class=\"category\">\n              \n            Category: {{product.category}}.\n            <p>Img {{product.imgFile}}</p>\n            <div v-bind:picture=\"product.imgFile\"><p>Picture: {{picture}}</p></div>\n            picture:<p>{{picture}}</p>\n            <p :gettest=\"getTest(product.imgFile)\">test: {{imgFile}}</p>\n            <!-- <img :src=\"requre(@/src\" alt=\"\"> -->\n            \n          </span>\n          \n          <span class=\"longDesc\">\n            Long description{{product.longDesc}}\n            </span>\n\n          <span class=\"price\">\n            Price: {{product.price}}\n          </span>        \n        </div> \n        <!-- <img :src=\"require('@/assets/'+product.imgFile)\" class=\"d-block w-100\" alt=\"b\"> -->\n        <!-- sidebar slut -->\n\n      <!--\n      <div id=\"carouselExampleIndicators\" class=\"carousel slide\" data-ride=\"carousel\">\n            \n\n            <ol class=\"carousel-indicators\">\n              <li data-target=\"#carouselExampleIndicators\" data-slide-to=\"0\" class=\"active\"></li>\n              <li data-target=\"#carouselExampleIndicators\" data-slide-to=\"1\"></li>\n              <li data-target=\"#carouselExampleIndicators\" data-slide-to=\"2\"></li>\n            </ol>\n\n            <div class=\"carousel-inner\">\n              \n              <div class=\"carousel-item active\">\n\n                <img :src=\"require('@/assets/'+product.imgFile)\" class=\"d-block w-100\" alt=\"b\">\n                </div>\n              \n\n          \n            <a class=\"carousel-control-prev\" @click=\"getPrev\" role=\"button\" data-slide=\"prev\">\n              <span class=\"carousel-control-prev-icon\" aria-hidden=\"true\"></span>\n              <span class=\"sr-only\">Previous</span>\n            </a>\n\n            <a class=\"carousel-control-next\" @click=\"getNext\" role=\"button\" data-slide=\"next\">\n              <span class=\"carousel-control-next-icon\" aria-hidden=\"true\"></span>\n              <span class=\"sr-only\">Next</span>\n            </a>\n           \n            \n          </div>\n        <div class=\"footer\"> <h1>footer</h1>:${{product.price}}</div>\n        \n      </div>\n      -->     \n  </div>\n  \n      \n</template>\n\n<script>\n// import Item from './Item.vue';\n//import {mapGetters} from 'vuex';\nimport {mapState} from 'vuex';\n\nexport default {\n  data(){\n    \n    return {\n      counter: 0,\n      current_product: {},\n      product: {},\n      imgFile: '',\n      list: [],\n      self: 'In ProductList: ',\n      size: 0,\n      picture: '',\n\n    }\n\n  },\n  \n\n  methods:{\n    getItem($event){\n    \n      console.log($event);\n      this.product = $event;\n    },\n\n    setSize($event){\n      this.size = $event;\n      console.log(\"setSize: \"+this.size);\n    },\n    getNext(){\n\n      console.log(this.counter);\n      console.log(\"getNext\", this.product);\n      this.counter = this.counter %  this.products.length;\n      this.product = this.products[this.counter];\n      \n      this.counter += 1;\n      console.log(this.product);\n    },\n    \n    getPrev(){\n      this.counter -= 1;\n\n      \n      if(this.counter < 0){\n        this.counter = this.products.length - 1;\n      }\n      console.log(this.counter);\n      console.log(this.product);\n\n      this.counter = this.counter % this.products.length;\n      this.product = this.product[this.counter];\n    },\n\n    getTest($event){\n      console.log(this.self + \"in getTest($event): \", $event);\n      this.imgFile = $event;\n      // let temp = '@/assets/'+$event.imgFile;\n      console.log(this.self + \"in getTest(this.imgFile) \");\n    },\n    func($event){\n      console.log(this.self + \"in func: \", $event)\n    }\n  },\n    components:{\n      // Item: Item,\n  },\n\n  computed:{\n    ... mapState(['products'])\n  },\n\n  watch:{\n    products:function(){\n      \n      console.log(this.self + \"watching: \", this.products);\n\n      this.product = this.products[this.counter];\n      this.size = this.products.length;\n    } \n  },\n\n    beforeCreate() {\n    console.log(\"in ProductList.vue: beforeCreate(): calling store\");\n    console.log(this);\n  \n    this.$store.dispatch('loadProducts');\n\n    console.log(\"In ProductList.vue:beforeCreate() \"  +\" 'this' is \"+this.self );\n    console.log(\"In ProductList.vue:beforeCreate(): and this.products is: \" , this.products);\n    },\n\n  created() {\n    console.log(this.self + \"in created \", this.products);\n    // console.log(this.self+ \" created(): lenght \", this.products.length);\n  },\n\n  beforeMount() {\n    console.log(this.self + \"before mount\", this.products.length);\n  },\n  mounted() {\n    console.log(this.self + \"mounted\", this.products);\n    // this.imgFile = this.products[this.counter].imgFile\n    \n    \n  },\n}\n</script>\n<style scoped>\n\n  * {\n    \n    padding:0;\n    margin:0;\n   border: 1px solid black;\n  }\n  body{\n    border: 1px solid black;\n    width: fit-content;\n    padding: 100px;\n  }\n\n\n  .container{\n    background-color: #eee;\n    border: none;\n    margin: 50px;\n    padding:20px;\n    width: 1100px;\n    height: 900px;\n    \n    border: 1px solid black;\n    display: grid;\n    grid-template-columns: 4fr 2fr;\n    grid-template-rows: .5fr 3fr 1fr;\n    gap: 10px;\n    grid-template-areas: \n      \" header    header\"\n       \"carousel  sidebar\"\n      \" footer    sidebar\";\n  }\n\n\n  .header{\n    grid-area:header;\n    border: 1px solid blue;\n  }\n  .carousel{\n    grid-area: carousel;\n    border: 1px solid black;\n  }  \n\n\n\n\n  .sidebar{\n    grid-area: sidebar;\n\n    display: grid;\n    border: 1px solid black;\n\n    row-gap: 10px;;\n    \n  }\n\n  .carousel-indicators{\n    border: 1px solid red;\n  }\n\n  carousel-item{\n    \n    height: fit-content;\n    width: fit-content;\n    \n  }\n  .footer{\n    grid-area: footer;\n    height: fit-content;\n    border: 1px solid green;\n  }\n\n  .img{\n    color:red;\n  }\n\n</style>\n"]}]}