\r\n\r\n    // Är non-synchronous okej - när - varför ?\r\n    // loadProducts({ commit }) {\r\n    //     Api.get(\"products\")\r\n    //         .then((result) => {\r\n    //             commit(\"SAVE_PRODUCTS\", result.data);\r\n    //         })\r\n    //         .catch((error) => {\r\n    //             throw new Error(`API ${error}`);\r\n    //         });\r\n    // },\r\n  },\r\n  modules: {\r\n  }\r\n})\r\n"]}]}